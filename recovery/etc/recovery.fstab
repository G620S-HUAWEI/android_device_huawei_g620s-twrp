/boot         emmc   /dev/block/bootdevice/by-name/boot           flags=display="boot"
/cache        ext4   /dev/block/bootdevice/by-name/cache          flags=display="cache"
/persist      ext4   /dev/block/bootdevice/by-name/persist
/data         ext4   /dev/block/bootdevice/by-name/userdata       length=-16384;flags=display="data"
/recovery     emmc   /dev/block/bootdevice/by-name/recovery
/misc         emmc   /dev/block/bootdevice/by-name/misc
/firmware     vfat   /dev/block/bootdevice/by-name/modem
/system       ext4   /dev/block/bootdevice/by-name/system         flags=display="system"

/external_sd  vfat   /dev/block/mmcblk1p1 /dev/block/mmcblk1      flags=display="external_sd";storage;wipeingui;removable
/usbdisk      vfat   /devices/platform/msm_hsusb                  flags=display="usbdisk";storage;removable
